{
  "name": "vinkel",
  "version": "1.0.2",
  "description": "Argular components lib",
  "main": "vinkel.umd.js",
  "jsnext:main": "vinkel.umd.js",
  "module": "vinkel.umd.js",
  "types": "vinkel.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy": "node tools/copy-package.js && node tools/cleanup-package.js",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc",
    "postbundle": "node tools/removecode.js",
    "build": "npm run bundle && npm run copy",
    "publish": "cd ./dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fivunlm/vinkel.git"
  },
  "keywords": [
    "angular"
  ],
  "author": "fivunlm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fivunlm/vinkel/issues"
  },
  "homepage": "https://github.com/fivunlm/vinkel#readme",
  "dependencies": {
    "@angular/core": "^4.2.4",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "del": "^3.0.0",
    "node-sass": "^4.5.3",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.4.4"
  }
}
